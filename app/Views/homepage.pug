extends layout
block menu
  +menu()
block content
  h1 Accueil
  br
  h3.mb-3 Les services que nous supportons pour moment
  .d-flex.justify-content-center
    ul.list-group.list-group-horizontal
      each service in services
        li
          a(href=service.url)
            img.service-icon-40.rounded.rounded-3(src=service.img alt=service.name)